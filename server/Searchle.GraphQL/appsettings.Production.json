{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AppConfig": {
    "Metadata": {
      "EnvironmentName": "PROD1"
    },
    "DictionaryConnectionConfig": {
      "ConnectionString": "Searchle:DictionaryConnectionString"
    },
    "Logging": {
      "LogLevel": "Debug",
      "LogSinks": [
        {
          "SinkType": "Console"
        },
        {
          "SinkType": "File",
          "Destination": "./bin/logs/searchle.graphql.log"
        }
      ]
    },
    "Secrets": {
      "Source": "Environment",
      "RootKey": "Searchle:RootKey",
      "Encrypted": false
    }
  }
}